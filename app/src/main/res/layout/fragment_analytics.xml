<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.questionnaire.ui.AnalyticsFragment">

    <android.support.v7.widget.Toolbar
            android:id="@+id/Question.Toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:layout_alignParentTop="true"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            android:background="#FFFFFF"
            app:titleTextColor="#000000"
            android:elevation="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Analytics.PointsGotten"
            android:layout_centerInParent="true" android:textSize="18sp" android:textColor="#000000"
            android:layout_marginBottom="10dp" android:fontFamily="@font/ubuntu_medium"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Analytics.MarksGotten"
            android:layout_below="@id/Analytics.PointsGotten" android:layout_centerHorizontal="true"
            android:textColor="#000000" android:fontFamily="@font/ubuntu_light"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Analytics.Exit" android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"/>

</RelativeLayout>
